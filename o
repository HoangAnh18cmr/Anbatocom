local A = game:GetService("TweenService")
local B = game:GetService("TeleportService")
local C = game.Players.LocalPlayer
local D

local function E(F)
    D = F:WaitForChild("HumanoidRootPart")
end

C.CharacterAdded:Connect(E)

if C.Character then
    E(C.Character)
end

local function G(H)
    local I = TweenInfo.new(1, Enum.EasingStyle.Linear)
    local J = {CFrame = CFrame.new(H)}
    local K = A:Create(D, I, J)
    K:Play()
end

local function L(M)
    local N = {}

    for _, O in pairs(workspace:GetDescendants()) do
        if O:IsA("Decal") and O.Texture == M then
            table.insert(N, O.Parent)
        end
    end

    if #N > 0 then
        table.sort(N, function(P, Q)
            local R = P:IsA("BasePart") and P.Position or (P.PrimaryPart and P.PrimaryPart.Position) or Vector3.new(0, 0, 0)
            local S = Q:IsA("BasePart") and Q.Position or (Q.PrimaryPart and Q.PrimaryPart.Position) or Vector3.new(0, 0, 0)
            return (D.Position - R).Magnitude < (D.Position - S).Magnitude
        end)

        for _, T in ipairs(N) do
            if T:IsA("BasePart") then
                local U = T.Position + Vector3.new(0, 0.1, 0)
                G(U)
                wait(0.3)
            elseif T:IsA("Model") and T:FindFirstChild("PrimaryPart") then
                local U = T.PrimaryPart.Position + Vector3.new(0, 0.1, 0)
                G(U)
                wait(0.3)
            end
        end
    end
end

local function V()
    local W = game.CoreGui.RobloxPromptGui.promptOverlay:FindFirstChild("ErrorPrompt")
    if W then
        task.wait(15)
        C:Kick("Sút bay Màu Roblox")
        task.wait(10)
        B:Teleport(game.PlaceId)
    end
end

while true do
    L("rbxassetid://6087969886")
    V()
    wait(1)
end
